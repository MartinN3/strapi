{
  "connection": "default",
  "collectionName": "components_social_media_metadata_event",
  "info": {
    "name": "event",
    "icon": "calendar"
  },
  "options": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "startDate": {
      "type": "datetime"
    },
    "endDate": {
      "type": "datetime"
    },
    "description": {
      "type": "text"
    },
    "cloudinaryImage": { "cloudinary": true, "type": "json" },
    "locationName": {
      "type": "string"
    },
    "streetAddress": {
      "type": "string"
    },
    "addressLocality": {
      "type": "string"
    },
    "postalCode": {
      "type": "string"
    },
    "addressCountry": {
      "type": "string"
    },
    "offerprice": {
      "type": "decimal"
    },
    "offerPriceCurrency": {
      "type": "enumeration",
      "enum": ["CZK", "EUR"]
    },
    "offersAvailability": {
      "type": "enumeration",
      "enum": [
        "inStock",
        "inStoreOnly",
        "limitedAvailability",
        "onlineOnly",
        "preOrder",
        "preSale",
        "soldOut",
        "discontinued"
      ]
    },
    "offerValidFrom": {
      "type": "datetime"
    },
    "performer": {
      "type": "string"
    }
  }
}
