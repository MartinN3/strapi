{
  "kind": "collectionType",
  "connection": "default",
  "collectionName": "downloadable_documents",
  "info": {
    "name": "Dokumenty ke stažení"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "title": {
      "type": "string"
    },
    "dateAdded": {
      "type": "date",
      "private": false
    },
    "attachment": {
      "cloudinary": true,
      "type": "json"
    },
    "entityStatus": {
      "type": "component",
      "repeatable": false,
      "component": "helpers.entity-status",
      "required": true
    },
    "seo": {
      "type": "component",
      "repeatable": false,
      "component": "helpers.seo",
      "required": true
    },
    "helpers": {
      "type": "component",
      "repeatable": false,
      "component": "helpers.helpers",
      "required": false
    }
  }
}
